==========================\n\n# ============================================================\n# 1ï¸âƒ£ VERIFY ENVIRONMENT\n# ============================================================\nimport torch, diffusers, transformers\n\nprint(\"Torch:\", torch.__version__)\nprint(\"Diffusers:\", diffusers.__version__)\nprint(\"Transformers:\", transformers.__version__)\nprint(\"CUDA:\", torch.cuda.is_available())\n\n\n# ============================================================\n# 2ï¸âƒ£ INSTALL PEFT (ONCE PER SESSION)\n# ============================================================\n!pip install -q peft\n\n\n# ============================================================\n# 3ï¸âƒ£ LOGIN TO HUGGINGFACE\n# ============================================================\nfrom huggingface_hub import notebook_login\nnotebook_login()\n\n\n# ============================================================\n# 4ï¸âƒ£ LOAD PIPELINE\n# ============================================================\nfrom diffusers import StableDiffusionPipeline\n\nDEVICE = \"cuda\"\nMODEL_ID = \"runwayml/stable-diffusion-v1-5\"\n\npipe = StableDiffusionPipeline.from_pretrained(\n    MODEL_ID,\n    torch_dtype=torch.float16,\n    safety_checker=None\n).to(DEVICE)\n\npipe.unet.enable_gradient_checkpointing()\n\npipe.vae.requires_grad_(False)\npipe.text_encoder.requires_grad_(False)\n\n\n# ============================================================\n# 5ï¸âƒ£ APPLY LoRA VIA PEFT (SUPPORTED WAY)\n# ============================================================\nfrom peft import LoraConfig, get_peft_model\n\nlora_config = LoraConfig(\n    r=4,\n    lora_alpha=4,\n    target_modules=[\"to_q\", \"to_k\", \"to_v\", \"to_out.0\"],\n    lora_dropout=0.05,\n    bias=\"none\",\n)\n\npipe.unet = get_peft_model(pipe.unet, lora_config)\npipe.unet.train()\n\n\n# ============================================================\n# 6ï¸âƒ£ DATASET\n# ============================================================\nfrom torch.utils.data import Dataset, DataLoader\nfrom torchvision import transforms\nfrom PIL import Image\nimport os\n\nDATA_DIR = \"/kaggle/input/schimmel/mold/\"\n\nclass SchimmelDataset(Dataset):\n    def __init__(self, folder, caption):\n        self.folder = folder\n        self.caption = caption\n        self.images = [\n            os.path.join(folder, f)\n            for f in os.listdir(folder)\n            if f.lower().endswith((\".jpg\", \".jpeg\", \".png\"))\n        ]\n\n        if len(self.images) == 0:\n            raise ValueError(\"âŒ No images found in dataset folder\")\n\n        print(f\"âœ… Using {len(self.images)} images with fixed caption\")\n\n        self.transform = transforms.Compose([\n            transforms.Resize((512, 512)),\n            transforms.ToTensor(),\n            transforms.Normalize([0.5], [0.5])\n        ])\n\n    def __len__(self):\n        return len(self.images)\n\n    def __getitem__(self, idx):\n        img_path = self.images[idx]\n        image = Image.open(img_path).convert(\"RGB\")\n        image = self.transform(image)\n        return image, self.caption\n\n\nimgs = {os.path.splitext(f)[0] for f in os.listdir(DATA_DIR)\n        if f.lower().endswith((\".jpg\", \".png\", \".jpeg\"))}\n\ntxts = {os.path.splitext(f)[0] for f in os.listdir(DATA_DIR)\n        if f.lower().endswith(\".txt\")}\n\nprint(\"Images:\", len(imgs))\nprint(\"Captions:\", len(txts))\nprint(\"Matched pairs:\", len(imgs & txts))\nprint(\"Images without captions:\", len(imgs - txts))\n\n\nCAPTION = \"schimmel mold on damp concrete wall, realistic texture\"\n\ndataset = SchimmelDataset(DATA_DIR, CAPTION)\nloader = DataLoader(dataset, batch_size=1, shuffle=True)\n\n\n\n# ============================================================\n# 7ï¸âƒ£ OPTIMIZER\n# ============================================================\noptimizer = torch.optim.AdamW(pipe.unet.parameters(), lr=5e-5)\n\n\n# ============================================================\n# 8ï¸âƒ£ TRAINING LOOP\n# ============================================================\nimport torch.nn.functional as F\n\nEPOCHS = 10\n\nfor epoch in range(EPOCHS):\n    for images, captions in loader:\n        images = images.to(DEVICE, dtype=torch.float16)\n        captions = list(captions)\n\n        latents = pipe.vae.encode(images).latent_dist.sample() * 0.18215\n        noise = torch.randn_like(latents)\n\n        timesteps = torch.randint(\n            0,\n            pipe.scheduler.config.num_train_timesteps,\n            (latents.size(0),),\n            device=DEVICE\n        ).long()\n\n        noisy_latents = pipe.scheduler.add_noise(latents, noise, timesteps)\n\n        encoder_hidden_states = pipe.text_encoder(\n            pipe.tokenizer(\n                captions,\n                padding=\"max_length\",\n                truncation=True,\n                max_length=77,\n                return_tensors=\"pt\"\n            ).input_ids.to(DEVICE)\n        )[0]\n\n        noise_pred = pipe.unet(\n            noisy_latents,\n            timesteps,\n            encoder_hidden_states\n        ).sample\n\n        loss = F.mse_loss(noise_pred, noise)\n        loss.backward()\n\n        optimizer.step()\n        optimizer.zero_grad()\n\n    print(f\"Epoch {epoch+1}/{EPOCHS} | Loss: {loss.item():.4f}\")\n\n\n# ============================================================\n# 9ï¸âƒ£ SAVE LoRA\n# ============================================================\nLORA_PATH = \"/kaggle/working/schimmel_lora\"\npipe.unet.save_pretrained(LORA_PATH)\nprint(\"LoRA saved to:\", LORA_PATH)\n\n\n# ============================================================\n# ğŸ”Ÿ GENERATE IMAGES\n# ============================================================\npipe.unet.eval()\n\nPROMPT = \"close-up photograph of schimmel mold on damp concrete wall, macro, realistic\"\nNEGATIVE_PROMPT = \"cartoon, illustration, blur, artifacts\"\n\nos.makedirs(\"synthetic_schimmel\", exist_ok=True)\n\nfor i in range(50):\n    img = pipe(\n        PROMPT,\n        negative_prompt=NEGATIVE_PROMPT,\n        num_inference_steps=30,\n        guidance_scale=6.5\n    ).images[0]\n\n    img.save(f\"synthetic_schimmel/schimmel_{i}.png\")\n\nprint(\"Synthetic images generated âœ…\")\n","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2025-12-29T01:27:47.299337Z","iopub.execute_input":"2025-12-29T01:27:47.300210Z","iopub.status.idle":"2025-12-29T01:42:29.169802Z","shell.execute_reply.started":"2025-12-29T01:27:47.300175Z","shell.execute_reply":"2025-12-29T01:42:29.169093Z"}},"outputs":[{"name":"stdout","text":"Torch: 2.8.0+cu126\nDiffusers: 0.35.2\nTransformers: 4.57.1\nCUDA: True\n","output_type":"stream"},{"output_type":"display_data","data":{"text/plain":"VBox(children=(HTML(value='<center> <img\\nsrc=https://huggingface.co/front/assets/huggingface_logo-noborder.svâ€¦","application/vnd.jupyter.widget-view+json":{"version_major":2,"version_minor":0,"model_id":"ef3470bdbc2549bbb4a7faf7f509a012"}},"metadata":{}},{"output_type":"display_data","data":{"text/plain":"Loading pipeline components...:   0%|          | 0/6 [00:00<?, ?it/s]","application/vnd.jupyter.widget-view+json":{"version_major":2,"version_minor":0,"model_id":"26d550a30d5641c5a42e54a72ebd5981"}},"metadata":{}},{"name":"stderr","text":"You have disabled the safety checker for <class 'diffusers.pipelines.stable_diffusion.pipeline_stable_diffusion.StableDiffusionPipeline'> by passing `safety_checker=None`. Ensure that you abide to the conditions of the Stable Diffusion license and do not expose unfiltered results in services or applications open to the public. Both the diffusers team and Hugging Face strongly recommend to keep the safety filter enabled in all public facing circumstances, disabling it only for use-cases that involve analyzing network behavior or auditing its results. For more information, please have a look at https://github.com/huggingface/diffusers/pull/254 .\n","output_type":"stream"},{"name":"stdout","text":"Images: 104\nC